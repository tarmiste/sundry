PKG="mc"
PKG_VERSION="4.8.20"
PKG_FILE="mc-4.8.20.tar.xz"
URL="http://ftp.midnight-commander.org/${PKG_FILE}"
MD5="7f808b01f3f7d9aa52152a9efb86dbca"
for i in PATCH{1..10}; do
   unset $i
done

( cat << "xEOFx"
./configure --prefix=/usr \
            --sysconfdir=/etc \
            --enable-charset &&
make
make install
cp -v doc/keybind-migration.txt /usr/share/mc
xEOFx
) > tmp
